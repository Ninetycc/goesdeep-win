---
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header";
import Footer from "../components/Footer";
import Biow from "../components/Biow.astro";
---

<Layout title="HUGE! VAX CLASS ACTION LAWSUIT! - AUSTRALIANS DEMAND JUSTICE.">
  <Header client:only="react" />
  <main class="container">
    <!-- Jumbotron -->
    <div
      class="bg-gradient-to-tr from-red-900 to-blue-900 h-96 w-full relative rounded-xl"
    >
      <img
        src="images/Med-Bed001.jpg"
        class="w-full h-full object-cover absolute mix-blend-overlay"
      />
      <div class="p-24">
        <h1 class="text-white text-6xl font-bold text-center">
          AUSTRALIANS DEMAND JUSTICE
        </h1>
        <h2 class="text-slate-100 text-3xl mt-5 text-center">
          HUGE! VAX CLASS ACTION LAWSUIT! - AUSTRALIANS DEMAND JUSTICE FOR THE
          GOVERNMENT'S TYRANNY!
        </h2>
      </div>
    </div>
    <!-- Jumbotron -->

    <!-- HUGE! VAX CLASS ACTION LAWSUIT! - AUSTRALIANS DEMAND JUSTICE FOR THE GOVERNMENT'S TYRANNY! -->
    <div
      class="block px-4 bg-white m-4 shadow-[0_2px_15px_-3px_rgba(0,0,0,0.07),0_10px_20px_-2px_rgba(0,0,0,0.04)] rounded-xl border-r border-b border-l border-gray-400 lg:border-l-0 lg:border-t lg:border-gray-400 rounded-b lg:rounded-b-none lg:rounded-r p-4 justify-between leading-normal"
    >
      <Biow />
      <p class="mb-4 text-base text-blue-600">
        HUGE! VAX CLASS ACTION LAWSUIT! - AUSTRALIANS DEMAND JUSTICE FOR THE
        GOVERNMENT'S TYRANNY!
      </p>

      <div
        class="content-center bg-white overflow-hidden bg-cover bg-no-repeat"
        data-te-ripple-init
        data-te-ripple-color="light"
      >
        <div
          class="embed-responsive embed-responsive-16by9 relative w-full overflow-hidden"
          style="padding-top: 56.25%"
        >
          <iframe
            class="embed-responsive-item absolute bottom-0 left-0 right-0 top-0 h-full w-full rounded-xl"
            src="https://www.bitchute.com/embed/9UDWjw6aLvWG/"
            allowfullscreen=""
            data-gtm-yt-inspected-2340190_699="true"
            id="240632615"></iframe>
        </div>
      </div>
      <div class="p-6 top-0 text-center">
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://banned.video/watch?id=64567870c9796709436c42c4"
            rel="noopener"
            target="_blank"
            ><span
              class="text-blue-500 text-base hover:text-lg hover:text-green-900"
              >HUGE! VAX CLASS ACTION LAWSUIT! - AUSTRALIANS DEMAND JUSTICE FOR
              THE GOVERNMENT'S TYRANNY!</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://www.rumormillnews.com/cgi-bin/forum.cgi?read=222141"
            rel="noopener"
            target="_blank"
            ><span class="text-red-700 delay-200 hover:text-green-900"
              >VAX CLASS ACTION LAWSUIT: Australians Demand Justice For Govt’s
              Medical Tyranny</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://www.infowars.com/posts/vax-class-action-lawsuit-australians-demand-justice-for-the-governments-medical-tyranny/"
            rel="noopener"
            target="_blank"
            ><span class="text-red-700 delay-200 hover:text-green-900"
              >VAX CLASS ACTION LAWSUIT: Australians Demand Justice For Govt’s
              Medical Tyranny</span
            >
          </a>
        </h5>
      </div>
    </div>
    <!-- HUGE! VAX CLASS ACTION LAWSUIT! - AUSTRALIANS DEMAND JUSTICE FOR THE GOVERNMENT'S TYRANNY! -->

    <Footer client:only="react" />
  </main>
</Layout>
