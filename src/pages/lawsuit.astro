---
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header";
import Footer from "../components/Footer";
---

<Layout title="HUGE! VAX CLASS ACTION LAWSUIT! - AUSTRALIANS DEMAND JUSTICE.">
  <Header client:only="react" />
  <main class="container">
    <!-- Jumbotron -->
    <div
      class="bg-gradient-to-tr from-red-900 to-blue-900 h-96 w-full relative"
    >
      <img
        src="images/Med-Bed001.jpg"
        class="w-full h-full object-cover absolute mix-blend-overlay"
      />
      <div class="p-24">
        <h1 class="text-white text-6xl font-bold text-center">
          AUSTRALIANS DEMAND JUSTICE
        </h1>
        <h2 class="text-slate-100 text-3xl mt-5 text-center">
          HUGE! VAX CLASS ACTION LAWSUIT! - AUSTRALIANS DEMAND JUSTICE FOR THE
          GOVERNMENT'S TYRANNY!
        </h2>
      </div>
    </div>
    <!-- Jumbotron -->

    <!-- VAX CLASS ACTION LAWSUIT! -->
    <div class="md:container md:mx-auto">
      <!-- ... -->

      <div class="p-10">
        <!--Card 1-->
        <div class="w-full lg:max-w-full lg:flex">
          <div
            class="h-48 lg:h-auto lg:w-48 flex-none bg-cover rounded-t lg:rounded-t-none lg:rounded-l text-center overflow-hidden"
          >
          </div>
          <div
            class="border-r border-b border-l border-gray-400 lg:border-l-0 lg:border-t lg:border-gray-400 bg-white rounded-b lg:rounded-b-none lg:rounded-r p-4 flex flex-col justify-between leading-normal"
          >
            <div class="mb-8">
              <div class="text-gray-900 font-bold text-xl mb-2">
                <a
                  href="https://www.bitchute.com/video/9UDWjw6aLvWG/"
                  rel="noopener"
                  target="_blank"
                  ><span
                    class="text-red-700 text-base hover:text-lg hover:text-green-900"
                    >HUGE! VAX CLASS ACTION LAWSUIT! - AUSTRALIANS DEMAND
                    JUSTICE FOR THE GOVERNMENT'S TYRANNY!</span
                  >
                </a>
              </div>
              <br class="text-gray-700 text-base" />...
            </div>
            <div class="flex items-center">
              <div
                class="embed-responsive embed-responsive-16by9 relative w-full overflow-hidden"
                style="padding-top: 56.25%"
              >
                <iframe
                  class="embed-responsive-item absolute bottom-0 left-0 right-0 top-0 h-full w-full"
                  src="https://www.bitchute.com/embed/9UDWjw6aLvWG/"
                  allowfullscreen=""
                  data-gtm-yt-inspected-2340190_699="true"
                  id="240632615"></iframe>
              </div>
            </div>
            <div class="text-gray-900 font-bold text-xl mb-2">
              <a
                href="https://www.rumormillnews.com/cgi-bin/forum.cgi?read=222141"
                rel="noopener"
                target="_blank"
                ><span class="text-red-700 delay-200 hover:text-green-900"
                  >VAX CLASS ACTION LAWSUIT: Australians Demand Justice For
                  Govt’s Medical Tyranny</span
                >
              </a><br />
              <a
                href="https://www.infowars.com/posts/vax-class-action-lawsuit-australians-demand-justice-for-the-governments-medical-tyranny/"
                rel="noopener"
                target="_blank"
                ><span class="text-red-700 delay-200 hover:text-green-900"
                  >VAX CLASS ACTION LAWSUIT: Australians Demand Justice For
                  Govt’s Medical Tyranny</span
                >
              </a><br />
              <a
                href="https://banned.video/watch?id=64567870c9796709436c42c4"
