---
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header";
import Footer from "../components/Footer";
import Movies from "../components/Movies.astro";
import Bluelist from "../components/Bluelist.astro";
import Rept from "../components/Rept.astro";
---

<Layout title="Kent Dunn">
  <Header client:only="react" />
  <main class="container">
    <!-- Jumbotron -->
    <div
      class="bg-gradient-to-tl from-pink-500 to-blue-800 h-96 w-full relative rounded-xl"
    >
      <img
        src="images/Med-Bed001.jpg"
        class="w-full h-full object-cover absolute mix-blend-overlay"
      />
      <div class="p-24">
        <h1
          class="text-white text-xl lg:text-[4rem] lg:leading-[4rem] font-bold text-center text-transform: uppercase"
        >
          Kent Dunn
        </h1>
        <h2 class="text-slate-100 text-xl mt-5 text-center">
          Dark Outpost 08.05.2022 Pelosi Double Sent To Taiwan! Special Guest
          Kent Dunn Doubles Down: "Charlie Ward Is Dead!"
        </h2>
      </div>
    </div>
    <!-- Jumbotron -->

    <!--  Dark Outpost 08.05.2022 Pelosi Double Sent To Taiwan! Special Guest Kent Dunn Doubles Down: "Charlie Ward Is Dead!" -->
    <div
      class="block px-4 bg-white m-4 shadow-[0_2px_15px_-3px_rgba(0,0,0,0.07),0_10px_20px_-2px_rgba(0,0,0,0.04)] rounded-xl border-r border-b border-l border-gray-400 lg:border-l-0 lg:border-t lg:border-gray-400 rounded-b lg:rounded-b-none lg:rounded-r p-4 justify-between leading-normal"
    >
      <Bluelist />
      <h2 id="kent">
        <p class="mb-4 text-base text-blue-900">
          Dark Outpost 08.05.2022 Pelosi Double Sent To Taiwan! Special Guest
          Kent Dunn Doubles Down: "Charlie Ward Is Dead!"
        </p>
      </h2>
      <div
        class="content-center bg-white overflow-hidden bg-cover bg-no-repeat"
        data-te-ripple-init
        data-te-ripple-color="light"
      >
        <div
          class="embed-responsive embed-responsive-16by9 relative w-full overflow-hidden"
          style="padding-top: 56.25%"
        >
          <iframe
            class="embed-responsive-item absolute bottom-0 left-0 right-0 top-0 h-full w-full rounded-xl"
            src="https://rumble.com/embed/v1c6tyx/?pub=166b2j"
            allowfullscreen=""
            data-gtm-yt-inspected-2340190_699="true"
            id="240632615"></iframe>
        </div>
      </div>
      <div class="p-6 top-0 text-center">
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a href="./"
            ><span
              class="text-red-700 bg-white text-base hover:text-lg hover:text-green-900"
              >GO BACK</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://rumble.com/v1eszw5-dark-outpost-live-08.05.2022-pelosi-double-sent-to-taiwan.html"
            rel="noopener"
            target="_blank"
            ><span
              class="text-green-700 bg-white text-base hover:text-lg hover:text-green-900"
              >Dark Outpost 08.05.2022 Pelosi Double Sent To Taiwan! Special
              Guest Kent Dunn Doubles Down: "Charlie Ward Is Dead!"</span
            >
          </a>
        </h5>
      </div>
    </div>
    <!--  Dark Outpost 08.05.2022 Pelosi Double Sent To Taiwan! Special Guest Kent Dunn Doubles Down: "Charlie Ward Is Dead!" -->

    <!--  Kerry Cassidy Interviews Charlie Ward -->
    <div
      class="block px-4 bg-white m-4 shadow-[0_2px_15px_-3px_rgba(0,0,0,0.07),0_10px_20px_-2px_rgba(0,0,0,0.04)] rounded-xl border-r border-b border-l border-gray-400 lg:border-l-0 lg:border-t lg:border-gray-400 rounded-b lg:rounded-b-none lg:rounded-r p-4 justify-between leading-normal"
    >
      <h2 id="ward">
        <p class="mb-4 text-base text-blue-900">
          Kerry Cassidy Interviews Charlie Ward
        </p>
      </h2>
      <div
        class="content-center bg-white overflow-hidden bg-cover bg-no-repeat"
        data-te-ripple-init
        data-te-ripple-color="light"
      >
        <div
          class="embed-responsive embed-responsive-16by9 relative w-full overflow-hidden"
          style="padding-top: 56.25%"
        >
          <iframe
            class="embed-responsive-item absolute bottom-0 left-0 right-0 top-0 h-full w-full rounded-xl"
            src="https://www.brighteon.com/embed/e4e9fdf1-038d-40e5-b928-474e7fcb2e30"
            allowfullscreen=""
            data-gtm-yt-inspected-2340190_699="true"
            id="240632615"></iframe>
        </div>
      </div>
      <div class="p-6 top-0 text-center">
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a href="./"
            ><span
              class="text-red-700 bg-white text-base hover:text-lg hover:text-green-900"
              >GO BACK</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://www.brighteon.com/e4e9fdf1-038d-40e5-b928-474e7fcb2e30"
            rel="noopener"
            target="_blank"
            ><span
              class="text-green-700 bg-white text-base hover:text-lg hover:text-green-900"
              >Kerry Cassidy Interviews Charlie Ward</span
            >
          </a>
        </h5>

        <Rept />
      </div>
    </div>
    <!--  Dark Outpost 08.05.2022 Pelosi Double Sent To Taiwan! Special Guest Kent Dunn Doubles Down: "Charlie Ward Is Dead!" -->

    <!-- Card title 2 -->
  </main>
</Layout>
<Footer client:only="react" />
