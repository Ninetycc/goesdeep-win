---
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header";
import Footer from "../components/Footer";
import Dmsolist from "../components/Dmsolist.astro";
import PharmakeiaCC from "../components/PharmakeiaCC.astro";
import Biow from "../components/Biow.astro";
import Lista from "../components/Lista.astro";
import Suramin from "../components/Suramin.astro";
import Barry from "../components/Barry.astro";
---

<Layout title="Pfizer">
