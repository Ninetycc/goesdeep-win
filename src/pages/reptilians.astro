---
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header";
import Footer from "../components/Footer";
import Rept from "../components/Rept.astro";
import Bluelist from "../components/Bluelist.astro";
---

<Layout title="Reptilians">
  <Header client:only="react" />
  <main class="container">
    <!-- Jumbotron -->
    <div
      class="bg-gradient-to-tl from-purple-900 to-sky-500 h-96 w-full relative rounded-xl"
    >
      <img
        src="images/Med-Bed001.jpg"
        class="w-full h-full object-cover absolute mix-blend-overlay rounded-xl"
      />
      <div class="p-24">
        <h1
          class="text-white text-xl lg:text-[4rem] lg:leading-[4rem] font-bold text-center text-transform: uppercase"
        >
          Reptilians
        </h1>
        <h2 class="text-slate-100 text-xl mt-5 text-center">
          Dr. Michael Salla — Shocking Evidence of Earth-Based Reptilians and
          Alien Technologies Revealed
        </h2>
      </div>
    </div>
    <!-- Jumbotron -->

    <!-- Shocking Evidence of Earth-Based Reptilians & Alien Technologies Revealed! -->
    <div
      class="block px-4 bg-white m-4 shadow-[0_2px_15px_-3px_rgba(0,0,0,0.07),0_10px_20px_-2px_rgba(0,0,0,0.04)] rounded-xl border-r border-b border-l border-gray-400 lg:border-l-0 lg:border-t lg:border-gray-400 rounded-b lg:rounded-b-none lg:rounded-r p-4 justify-between leading-normal"
    >
      <Bluelist />
      <h2 id="reptilians">
        <p class="mb-4 text-base text-blue-900">
          Shocking Evidence of Earth-Based Reptilians & Alien Technologies
          Revealed!
        </p>
      </h2>
      <div
        class="content-center bg-white overflow-hidden bg-cover bg-no-repeat"
        data-te-ripple-init
        data-te-ripple-color="light"
      >
        <div
          class="embed-responsive embed-responsive-16by9 relative w-full overflow-hidden"
          style="padding-top: 56.25%"
        >
          <iframe
            class="embed-responsive-item absolute bottom-0 left-0 right-0 top-0 h-full w-full rounded-xl"
            src="https://rumble.com/embed/v4bb8fw/?pub=166b2j"
            allowfullscreen=""
            data-gtm-yt-inspected-2340190_699="true"
            id="240632615"></iframe>
        </div>
      </div>
      <div class="p-6 top-0 text-center">
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a href="./#trash"
            ><span
              class="text-red-700 bg-white text-base hover:text-lg hover:text-green-900"
              >GO BACK</span
            >
          </a>
        </h5>

        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://operationdisclosureofficial.com/2024/02/17/dr-michael-salla-shocking-evidence-of-earth-based-reptilians-and-alien-technologies-revealed/"
            rel="noopener"
            target="_blank"
            ><span
              class="text-green-500 bg-white text-base hover:text-lg hover:text-green-900"
              >Dr. Michael Salla — Shocking Evidence of Earth-Based Reptilians
              and Alien Technologies Revealed</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://rumble.com/v4dwife-shocking-evidence-of-earth-based-reptilians-and-alien-technologies-revealed.html"
            rel="noopener"
            target="_blank"
            ><span
              class="text-green-900 bg-white text-base hover:text-lg hover:text-green-900"
              >Dr. Michael Salla — Shocking Evidence of Earth-Based Reptilians
              and Alien Technologies Revealed</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://twitter.com/MichaelSalla"
            rel="noopener"
            target="_blank"
            ><span
              class="text-sky-500 bg-white text-base hover:text-lg hover:text-green-900"
              >@MichaelSalla</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a href="./clif#chaos"
            ><span
              class="text-red-700 font-bold hover:text-2xl hover:text-green-900"
              >Clif High Interview With Sarah Westall - Aliens, Antarctica, the
              Big Event and even more Chaos</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a href="./howe"
            ><span
              class="text-blue-900 text-base hover:text-lg hover:text-red-700"
              >Linda Moulton Howe</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a href="./howe"
            ><span
              class="text-blue-900 text-base hover:text-lg hover:text-red-700"
              >Dr. Michael Salla</span
            >
          </a>
        </h5>
        <p class="mb-4 text-base text-neutral-600 dark:text-neutral-200">...</p>
        <Rept />
      </div>
    </div>
    <!-- Shocking Evidence of Earth-Based Reptilians & Alien Technologies Revealed! -->

    <!-- Footer -->
  </main>
</Layout>
<Footer client:only="react" />
