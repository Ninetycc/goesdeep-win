---
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header";
import Footer from "../components/Footer";
import Rept from "../components/Rept.astro";
import Bluelist from "../components/Bluelist.astro";
import Gone from "../components/Gone.astro";
import Israel from "../components/Israel.astro";
---

<Layout title="SGT Pattie Brassard">
  <Header client:only="react" />

  <main class="container">
    <!-- Jumbotron -->
    <div
      class="bg-gradient-to-tr from-red-800 to-fuchsia-700 h-96 w-full relative rounded-xl"
    >
      <img
        src="images/Med-Bed001.jpg"
        class="w-full h-full object-cover absolute mix-blend-overlay"
      />
      <div class="p-24">
        <h1
          class="text-white text-xl lg:text-[4rem] lg:leading-[4rem] font-bold text-center"
        >
          SGT Pattie Brassard
        </h1>
        <h2 class="text-slate-100 sm:text-2xl md:text-3xl mt-5 text-center">
          SGT Pattie Brassard – Elections, Solar Winds, Current Events, and Q &
          A
        </h2>
      </div>
    </div>
    <!-- Jumbotron -->

    <!-- SGT Pattie Brassard – Elections, Solar Winds, Current Events, and Q & A -->
    <div
      class="block px-4 bg-white m-4 shadow-[0_2px_15px_-3px_rgba(0,0,0,0.07),0_10px_20px_-2px_rgba(0,0,0,0.04)] rounded-xl border-r border-b border-l border-gray-400 lg:border-l-0 lg:border-t lg:border-gray-400 rounded-b lg:rounded-b-none lg:rounded-r p-4 justify-between leading-normal"
    >
      <Bluelist />
      <h2 id="eggs">
        <p class="mb-4 text-base text-green-700">
          SGT Pattie Brassard – Elections, Solar Winds, Current Events, and Q &
          A
        </p>
      </h2>
      <div
        class="content-center bg-white overflow-hidden bg-cover bg-no-repeat"
        data-te-ripple-init
        data-te-ripple-color="light"
      >
        <div
          class="embed-responsive embed-responsive-16by9 relative w-full overflow-hidden"
          style="padding-top: 56.25%"
        >
          <iframe
            class="embed-responsive-item absolute bottom-0 left-0 right-0 top-0 h-full w-full rounded-xl"
            src="https://rumble.com/embed/vi4rxe/?pub=166b2j"
            allowfullscreen=""
            data-gtm-yt-inspected-2340190_699="true"
            id="240632615"></iframe>
        </div>
      </div>
      <div class="p-6 top-0 text-center">
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a href="./invisible-war"
            ><span
              class="text-red-700 bg-white text-base hover:text-lg hover:text-green-900"
              >GO BACK</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://rumble.com/vkqy2o-sgt-pattie-brassard-elections-solar-winds-current-events-and-q-and-a.html"
            rel="noopener"
            target="_blank"
            ><span
              class="text-green-700 text-base hover:text-lg hover:text-purple-900"
              >SGT Pattie Brassard – Elections, Solar Winds, Current Events, and
              Q & A (1:27:40)(1:30:10 eggs)</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://odysee.com/SGT-Pattie-Brassard-%E2%80%93-Elections,-Solar-Winds,-Current-Events,-and-Q---A:0"
            rel="noopener"
            target="_blank"
            ><span
              class="text-pink-500 text-base hover:text-lg hover:text-yellow-500"
              >SGT Pattie Brassard – Elections, Solar Winds, Current Events, and
              Q & A (1:27:40)(1:30:10 eggs)</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a href="./mcallister#dulce"
            ><span
              class="text-orange-500 text-base hover:text-lg hover:text-purple-900"
              >The Horrors Of Dulce [SD 360p]</span
            >
          </a>
        </h5>

        <Rept />
        <a href="./monsters">
          <p class="text-left text-yellow-100 hover:text-blue-300">
            Monsters Inc
          </p></a
        >
