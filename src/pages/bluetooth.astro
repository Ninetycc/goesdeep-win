---
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header";
import Footer from "../components/Footer";
import Rept from "../components/Rept.astro";
import Bluelist from "../components/Bluelist.astro";
import Bibles from "../components/Bibles.astro";
import Nagas from "../components/Nagas.astro";
import Diana from "../components/Diana.astro";
import Rundown from "../components/Rundown.astro";
import Gone from "../components/Gone.astro";
import Power from "../components/Power.astro";
import Qthestormm from "../components/Qthestormm.astro";
import DULCEL from "../components/DULCEL.astro";
import Lista from "../components/Lista.astro";
---

<Layout title="EVENT RUNDOWN">
  <Header client:only="react" />
  <main class="container">
    <!-- Jumbotron -->
    <div
      class="bg-gradient-to-tl from-blue-900 to-sky-400 h-96 w-full relative rounded-xl"
    >
      <img
        src="images/Med-Bed001.jpg"
        class="w-full h-full object-cover absolute mix-blend-overlay"
      />
      <div class="p-24">
        <h1
          class="text-white text-xl lg:text-[4rem] lg:leading-[4rem] font-bold text-center text-transform: uppercase"
        >Bluetooth LE Audio</h1>
        <h2 class="text-slate-100 text-xl mt-5 text-center">Starkey Edge AI Detailed Hearing Aid Review</h2>
      </div>
    </div>
    <!-- Jumbotron -->

        <!-- Starkey Edge AI Detailed Hearing Aid Review -->
    <div
      class="block px-4 bg-white m-4 shadow-[0_2px_15px_-3px_rgba(0,0,0,0.07),0_10px_20px_-2px_rgba(0,0,0,0.04)] rounded-xl border-r border-b border-l border-gray-400 lg:border-l-0 lg:border-t lg:border-gray-400 rounded-b lg:rounded-b-none lg:rounded-r p-4 justify-between leading-normal"
    >
      <Lista />
      <h2 id="starkey">
        <p class="mb-4 text-base text-left font-bold text-black">Starkey Edge AI Detailed Hearing Aid Review</p>
 
      </h2>
      <div
        class="content-center bg-white overflow-hidden bg-cover bg-no-repeat"
        data-te-ripple-init
        data-te-ripple-color="light"
      >
        <div
          class="embed-responsive embed-responsive-16by9 relative w-full overflow-hidden"
          style="padding-top: 56.25%"
        >
          <iframe
            class="embed-responsive-item absolute bottom-0 left-0 right-0 top-0 h-full w-full rounded-xl"
            src="https://www.youtube.com/embed/EG7SoW4lp0I?si=tZpDepJWF9Xxx7Lm&amp;start=900"
            allowfullscreen=""
            data-gtm-yt-inspected-2340190_699="true"
            id="240632615"></iframe>
        </div>
      </div>
      <div class="p-6 top-0 text-center">
        <LEAudio />
   
      </div>
    </div>
    <!-- Starkey Edge AI Detailed Hearing Aid Review -->

    <!-- Hearing Aid Bluetooth LE Audio Doctor Cliff, AuD -->
    <div
      class="block px-4 bg-white m-4 shadow-[0_2px_15px_-3px_rgba(0,0,0,0.07),0_10px_20px_-2px_rgba(0,0,0,0.04)] rounded-xl border-r border-b border-l border-gray-400 lg:border-l-0 lg:border-t lg:border-gray-400 rounded-b lg:rounded-b-none lg:rounded-r p-4 justify-between leading-normal"
    >
      
      <h2 id="leaudio">
        <p class="mb-4 text-base text-left font-bold text-black">Hearing Aid Bluetooth LE Audio Doctor Cliff, AuD</p>
     
      </h2>
      <div
        class="content-center bg-white overflow-hidden bg-cover bg-no-repeat"
        data-te-ripple-init
        data-te-ripple-color="light"
      >
        <div
          class="embed-responsive embed-responsive-16by9 relative w-full overflow-hidden"
          style="padding-top: 56.25%"
        >
          <iframe
            class="embed-responsive-item absolute bottom-0 left-0 right-0 top-0 h-full w-full rounded-xl"
            src="https://www.youtube.com/embed/OUHCdlnQekU?si=YvsOImbRVqJtMhgu"
            allowfullscreen=""
            data-gtm-yt-inspected-2340190_699="true"
            id="240632615"></iframe>
        </div>
      </div>
      <div class="p-6 top-0 text-center">
  <LEAudio />
      </div>
    </div>
    <!-- Hearing Aid Bluetooth LE Audio Doctor Cliff, AuD -->

    <!-- AN-OLDER-CODE -->
  </main>
</Layout>
<Footer client:only="react" />
