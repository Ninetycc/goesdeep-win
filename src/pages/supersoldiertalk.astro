      <div class="p-6 top-0 text-center">
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://rumble.com/v326qpc-super-soldier-talk-rainetta-jones-and-panel-guest-the-spider-queen-invasion.html"
            rel="noopener"
            target="_blank"
            ><span
              class="text-green-900 text-base hover:text-lg hover:text-red-700"
              >Super Soldier Talk – Rainetta Jones and Panel Guest – The Spider
              Queen Invasion (@22:26 Jahbulon)</span
            >
          </a>
        </h5>

        <a href="./monsters#part022">
          <p class="text-left text-yellow-100 hover:text-blue-300">
            Monsters Inc
          </p></a
        >
      </div>
    </div>
    <!-- Card title 2 -->

    <Footer client:only="react" />
  </main>
</Layout>
