---
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header";
import Footer from "../components/Footer";
import Bluelist from "../components/Bluelist.astro";
import Nonhuman from "../components/Nonhuman.astro";
import Rept from "../components/Rept.astro";
import Nagas from "../components/Nagas.astro";
import Contact from "../components/Contact.astro";
import Cosmic from "../components/Cosmic.astro";
---

<Layout title="Dwight ‘Ike’ Eisenhower">
  <Header client:only="react" />
  <main class="container">
    <!-- Jumbotron -->
    <div
      class="bg-gradient-to-tr from-purple-900 to-pink-500 h-96 w-full relative rounded-xl"
    >
      <img
        src="images/Med-Bed001.jpg"
        class="w-full h-full object-cover absolute mix-blend-overlay rounded-xl"
      />
      <div class="p-24">
        <h1
          class="text-white text-xl lg:text-[4rem] lg:leading-[4rem] font-bold text-center text-transform: uppercase"
        >
          Dwight ‘Ike’ Eisenhower
        </h1>
        <h2 class="text-slate-100 text-xl mt-5 text-center">
          IKE’S role in agreements reached with Aliens Revealed by Great
          Granddaughter in New Book
        </h2>
      </div>
    </div>
    <!-- Jumbotron -->

    <!-- IKE’S role in agreements reached with Aliens Revealed by Great Granddaughter in New Book -->
    <div
      class="block px-4 bg-white m-4 shadow-[0_2px_15px_-3px_rgba(0,0,0,0.07),0_10px_20px_-2px_rgba(0,0,0,0.04)] rounded-xl border-r border-b border-l border-gray-400 lg:border-l-0 lg:border-t lg:border-gray-400 rounded-b lg:rounded-b-none lg:rounded-r p-4 justify-between leading-normal"
    >
      <Bluelist />
      <Contact />
      <h2 id="ike">
        <p class="mb-4 text-base text-blue-900">
          IKE’S role in agreements reached with Aliens Revealed by Great
          Granddaughter in New Book
        </p>
      </h2>

      <div
        class="content-center bg-white overflow-hidden bg-cover bg-no-repeat"
        data-te-ripple-init
        data-te-ripple-color="light"
      >
        <div
          class="embed-responsive embed-responsive-16by9 relative w-full overflow-hidden"
          style="padding-top: 56.25%"
        >
          <iframe
            class="embed-responsive-item absolute bottom-0 left-0 right-0 top-0 h-full w-full rounded-xl"
            src="https://rumble.com/embed/v4gj0ic/?pub=166b2j"
            allowfullscreen=""
            data-gtm-yt-inspected-2340190_699="true"
            id="240632615"></iframe>
        </div>
      </div>
      <div class="p-6 top-0 text-center">
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a href="./"
            ><span
              class="text-red-700 bg-white text-base hover:text-lg hover:text-green-900"
              >GO BACK</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://rumble.com/v2eb9po-cosmic-interstellar-human-trafficking-with-tony-rodrigues.html"
            rel="noopener"
            target="_blank"
            ><span
              class="text-green-500 bg-white text-base hover:text-lg hover:text-purple-900"
              >Cosmic Interstellar Human Trafficking with Tony Rodrigues!</span
            >
          </a>
        </h5>
        <Cosmic />
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://rumble.com/v4j493u-ikes-role-in-agreements-reached-with-aliens-revealed-by-great-granddaughter.html"
            rel="noopener"
            target="_blank"
            ><span
              class="text-green-700 bg-white text-base hover:text-lg hover:text-green-900"
              >IKE’S role in agreements reached with Aliens Revealed by Great
              Granddaughter in New Book</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://rumble.com/c/ExopoliticsToday"
            rel="noopener"
            target="_blank"
            ><span
              class="text-green-700 bg-white text-base hover:text-lg hover:text-green-900"
              >Exopolitics Today with Dr. Michael Salla</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="https://michaelsalla.com/2024/03/14/ikes-role-in-agreements-reached-with-aliens-revealed-by-great-granddaughter-in-new-book/"
            rel="noopener"
            target="_blank"
            ><span
              class="text-green-700 bg-white text-base hover:text-lg hover:text-green-900"
              >IKE’S role in agreements reached with Aliens Revealed by Great
              Granddaughter in New Book</span
            >
          </a>
        </h5>
        <h5
          class="mb-2 text-xl font-medium leading-tight text-neutral-800 dark:text-neutral-50"
        >
          <a
            href="http://ufoevidence.org/documents/doc1154.htm"
            rel="noopener"
            target="_blank"
            ><span
              class="text-green-700 bg-white text-base hover:text-lg hover:text-green-900"
              >Ike and the Alien Ambassadors (Eisenhower)</span
            >
          </a>
        </h5>
        <Nagas />
        <Nonhuman />
        <Rept />
      </div>
    </div>
    <!-- IKE’S role in agreements reached with Aliens Revealed by Great Granddaughter in New Book -->

    <!-- Card title 2 -->
  </main>
</Layout>
<Footer client:only="react" />
